{
  "name": "otel/wordpress",
  "description": "An example of auto instrumentation of wordpress with opentelemetry",
  "type": "project",
  "require": {
    "open-telemetry/opentelemetry-auto-wordpress": "^0.1.1",
    "open-telemetry/sdk": "^1.7",
    "open-telemetry/exporter-otlp": "^1.3",
    "symfony/http-client": "^7.3"
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": true,
      "tbachert/spi": true
    }
  }
}
